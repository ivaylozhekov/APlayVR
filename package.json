{
  "name": "react-webpack-babel-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement) and Babel.",
  "keywords": [
    "react",
    "webpack",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikpe/react-webpack-babel-starter.git"
  },
  "bugs": {
    "url": "https://github.com/vikpe/react-webpack-babel-starter/issues"
  },
  "homepage": "https://github.com/vikpe/react-webpack-babel-starter",
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod",
    "clean-dist": "rimraf -f -r -d dist",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "jshint",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "concurrent --kill-others \"npm run start-dev-backend\" \"npm run start-dev\"",
    "start2": "concurrent --kill-others \"npm run start-prod\" \"npm run start-dev-backend\"",
    "start3": "concurrent --kill-others \"npm run start-dev\" \"npm run serve-static\"",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev --host 0.0.0.0",
    "start-dev-backend": "better-npm-run start-dev-backend",
    "start-prod": "npm run build && node express.js",
    "serve-static": "node expressStatic.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "betterScripts": {
    "start-dev-backend": {
      "command": "node ./bin/backend.js",
      "env": {
        "NODE_PATH": "./backend",
        "NODE_ENV": "development",
        "PORT": 3010
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.3",
    "jshint": "^2.9.5",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.4",
    "react": "^16.3.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "aframe": "^0.8.2",
    "aframe-leap-hands": "^0.7.2",
    "aframe-orbit-controls-component-2": "^0.1.14",
    "aframe-react": "^4.4.0",
    "body-parser": "^1.18.2",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "express-bearer-token": "^2.1.1",
    "express-session": "^1.15.6",
    "http-proxy": "^1.17.0",
    "lodash": "^4.17.10",
    "piping": "^1.0.0-rc.4",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "request-promise-native": "^1.0.5",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0",
    "socket.io-redis": "^5.2.0"
  },
  "postcss": {}
}
